XF86ScreenSaver
        slock
XF86AudioMute 
        amixer -q set 'Master' toggle
XF86AudioLowerVolume
        amixer -q set 'Master' 5%-
XF86AudioRaiseVolume
        amixer -q set 'Master' 5%+

super + XF86AudioMute 
        xbacklight -set 0
super + XF86AudioLowerVolume
        xbacklight -dec 5
super + XF86AudioRaiseVolume
        xbacklight -inc 5

XF86AudioPrev
        mpc prev
XF86AudioNext
        mpc next
XF86AudioStop
        mpc stop
XF86AudioPlay
        mpc toggle

super + Print
        sleep 0.2 && xdotool key XF86Ungrab && scrot --line style=dash,width=2 --select -e 'mv $f ~/Pictures/Captures'
Print
        sleep 0.2 && xdotool key XF86Ungrab && scrot -e 'mv $f ~/Pictures/Captures' 

